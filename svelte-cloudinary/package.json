{
	"name": "svelte-cloudinary",
	"author": "Matias Hernandez <hola@matiashernandez.dev>",
	"version": "1.0.0-beta.9",
	"license": "MIT",
	"scripts": {
		"dev": "node scripts/gen-metadata.js && vite dev",
		"build": "node scripts/gen-metadata.js && vite build && npm run package",
		"preview": "vite preview",
		"package": "node scripts/gen-metadata.js && svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "cp ../README.md . && npm run package",
		"postpublish": "rm ./README.md",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		".package.json": "./package.json"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "2.2.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"jest-dom": "4.0.0",
		"jsdom": "22.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unpic": "3.6.1",
		"vite": "^4.3.0",
		"vite-plugin-transform": "2.0.1",
		"vitest": "^0.25.3"
	},
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@cloudinary-util/url-loader": "3.5.1",
		"@cloudinary-util/util": "2.0.1",
		"@unpic/core": "0.0.24",
		"@unpic/svelte": "0.0.25"
	}
}
