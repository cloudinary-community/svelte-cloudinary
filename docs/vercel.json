
{
  "rewrites": [
    {
      "source": "/components/:path(.*)",
      "destination": "/:path*"
    },
    {
      "source": "/helpers/:path(.*)",
      "destination": "/:path*"
    },
    {
      "source": "/use-cases/:path(.*)",
      "destination": "/guides/:path*"
    },
    {
      "source": "/resources",
      "destination": "/"
    }
  ]
}

