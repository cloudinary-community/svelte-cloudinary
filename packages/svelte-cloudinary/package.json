{
	"name": "svelte-cloudinary",
	"version": "2.0.0-beta.2",
	"contributors": [
		"Willow (GHOST) <ghostdevbusiness@gmail.com> (https://ghostdev.xyz)",
		"Colby Fayock <hello@colbyfayock.com>",
		"Matias Hernandez <hola@matiashernandez.dev>"
	],
	"license": "MIT",
	"scripts": {
		"dev": "svelte-package --watch --input src",
		"build": "svelte-package --input src",
		"prepublishOnly": "node scripts/version.js",
		"postinstall": "node scripts/version.js",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && publint",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.0.0 || ^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@testing-library/svelte": "5.2.1",
		"@types/node": "^22.1.0",
		"desm": "^1.3.1",
		"happy-dom": "14.12.3",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.9",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"svelte-preprocess": "^6.0.2",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"unpic": "^3.18.0",
		"vitest": "^2.0.5"
	},
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@cloudinary-util/types": "^1.2.0",
		"@cloudinary-util/url-loader": "^5.6.0",
		"@cloudinary-util/util": "^3.2.0",
		"@unpic/svelte": "0.0.53"
	}
}