{
	"name": "svelte-cloudinary",
	"author": "Matias Hernandez <hola@matiashernandez.dev>",
	"version": "1.0.3",
	"license": "MIT",
	"scripts": {
		"dev": "node scripts/gen-metadata.js && vite dev",
		"build": "node scripts/gen-metadata.js && vite build && npm run package",
		"preview": "vite preview",
		"package": "node scripts/gen-metadata.js && svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "cp ../README.md . && npm run package",
		"postpublish": "rm ./README.md",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		".package.json": "./package.json"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.0.0 || ^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.2",
		"@sveltejs/vite-plugin-svelte": "2.2.0",
		"@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
		"@tailwindcss/typography": "^0.5.9",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@testing-library/user-event": "14.4.3",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.31.0",
		"happy-dom": "10.5.2",
		"highlight.js": "^11.8.0",
		"jsdom": "22.0.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"postcss-nesting": "^12.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.12",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.4",
		"svelte-highlight": "^7.3.0",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"unpic": "3.6.1",
		"vite": "^4.3.9",
		"vite-plugin-transform": "2.0.1",
		"vitest": "^0.25.8",
		"vitest-svelte-kit": "0.0.7"
	},
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@cloudinary-util/url-loader": "3.9.0",
		"@cloudinary-util/util": "2.1.0",
		"@unpic/core": "0.0.24",
		"@unpic/svelte": "0.0.25",
		"cloudinary": "^1.32.0"
	}
}
